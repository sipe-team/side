{
  "name": "@sipe-team/reset",
  "description": "Reset for Sipe Design System",
  "version": "0.1.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sipe-team/side"
  },
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./reset.css": "./src/reset.css"
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "lint:biome": "pnpm exec biome lint",
    "lint:eslint": "pnpm exec eslint",
    "test": "vitest",
    "typecheck": "tsc"
  },
  "dependencies": {},
  "devDependencies": {
    "@storybook/addon-essentials": "catalog:",
    "@storybook/addon-interactions": "catalog:",
    "@storybook/addon-links": "catalog:",
    "@storybook/blocks": "catalog:",
    "@storybook/react": "catalog:",
    "@storybook/react-vite": "catalog:",
    "@storybook/test": "catalog:",
    "@types/react": "^18.3.12",
    "react": "^18.3.1",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "vitest-browser-react": "catalog:"
  },
  "peerDependencies": {
    "react": ">= 18",
    "react-dom": ">= 18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com",
    "exports": {
      ".": {
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/index.cjs"
        }
      },
      "./reset.css": "./dist/reset.css"
    }
  },
  "sideEffects": false
}
